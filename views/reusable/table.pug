table
  thead
    - var columnNo = 0;
    tr
      each value in columnArray
        if columnNo == 0
          th(class="id")= value
        if columnNo > 0
          th= value
        - columnNo += 1
    each element in rowArray
      - var rowCellNo = 0;
      tr
        each item, index in element
            - console.log(index, item)
            if rowCellNo == 0
              td(class="id")= item
            else if rowCellNo == 1
              if title == "Books" || title == "Loans"
                td
                  a(href=bookHrefPath + '/' + element.id)= item
              if title == "Patrons"
                td
                  a(href=patronHrefPath + '/' + element.id)= item
            else if rowCellNo == 2
               if title == "Loans"
                 td
                   a(href=patronHrefPath)= item
               else
                 td= item
            else if rowCellNo == 6
                if title == "Loans" && item === 'Return Book'
                 td
                   a(href=actionHrefPath + '/' + element.id class="button")= item
                else if actionHrefPath
                  td
                    a(href=actionHrefPath + '/' + element.id)= item
            else
                td= item
            - rowCellNo += 1
